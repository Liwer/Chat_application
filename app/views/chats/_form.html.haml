= form_for @chat do |f|
  - if @chat.errors.any?
    #error_explanation
      %h2= "#{pluralize(@chat.errors.count, "error")} prohibited this chat from being saved:"
      %ul
        - @chat.errors.full_messages.each do |msg|
          %li= msg
  = f.collection_check_boxes(:user_ids, @users, :id, :username)
  %br 
  = f.label :name
  = f.text_field :name

  .actions
    = f.submit 'Save'
